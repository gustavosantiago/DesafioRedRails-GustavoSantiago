<div class="row">
  <% @users.each do |user| %>
    <div class="col-md-4">
      <div class="thumbnail">
        <%= image_tag user.profile.avatar.url(:thumb), class: "text-center" %> <br />
        <h3 class="text-center"><strong><%= link_to "#{user.profile.name}", user_profile_path(user) %><br /> </h3>
        <p class="text-center"><%= user.profile.bio %></p>
      </div>
    </div>
  <% end %>
</div>
